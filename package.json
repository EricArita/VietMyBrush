  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "build": "yarn lint && shx rm -rf ./dist && yarn build:less && next build ./src/client && yarn build:js && yarn copyToDist",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps --ignore src/**/*.test.ts",
    "build:js-test": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps",
    "build:less": "shx mkdir ./dist && shx mkdir ./dist/client && shx cp -R ./src/client/custom-antd.less ./dist/client/",
    "build:types": "tsc --project ./tsconfig.json --emitDeclarationOnly",
    "copyToDist": "node ./dist/server/scripts/copyToDist",
    "debug": "NODE_ENV=develop NODE_CONFIG_DIR=src/server/config nodemon --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/index.js | ./node_modules/.bin/bunyan",
    "dev": "yarn watch & ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=develop NODE_CONFIG_DIR=src/server/config nodemon --watch ./dist ./dist/index.js -p $PORT | ./node_modules/.bin/bunyan",
    "dev:fe": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=develop NODE_CONFIG_DIR=src/server/config node ./dist/index.js -p $PORT | ./node_modules/.bin/bunyan",
    "docker-debug": "docker-compose up",
    "lint": "./node_modules/.bin/tslint -c ./tslint.json -p ./tsconfig.json",
    "npm": "yarn build && yarn test && yarn publish",
    "start": "./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=production  NODE_CONFIG_DIR=dist/server/config node ./dist/index.js -p $PORT",
    "startWindow": "SET NODE_ENV=production NODE_CONFIG_DIR=dist/server/config && node ./dist/index.js -p $PORT && --watch ./src",
    "startB": "yarn build && yarn start",
    "test": "yarn build:js-test && NODE_ENV=test NODE_CONFIG_DIR=src/server/config nyc mocha dist/**/*.test.js",
    "testB": "yarn build && yarn test",
    "verify": "yarn build",
    "watch": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps --watch",
    "seedDb": "node ./dist/server/scripts/seedDb",
    "initDefaultRoles": "node ./dist/server/scripts/initDefaultRoles",
    "dev:window": "nodemon --watch ./src --exec \"ts-node -r tsconfig-paths/register\" ./src/index.ts | ./node_modules/.bin/bunyan"
  },
  "types": "dist/index.d.ts",
  "version": "0.1.0"
